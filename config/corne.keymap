#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>

/ {
        keymap {
                compatible = "zmk,keymap";

                default_layer {
// ------------------------------------------------------------------------------------------------------------------------------------------------------------
// |   TAB    |     Q     |     W     |     F     |     P     |     B     |          |      J     |     L    |     U     |     Y     |     ;     |    BSPC    |
// ------------------------------------------------------------------------------------------------------------------------------------------------------------
// |   CTRL   |     A     |     R     |     S     |     T     |     G     |          |      M     |     N    |     E     |     I     |     O     |    RET     |
// ------------------------------------------------------------------------------------------------------------------------------------------------------------
// |   SHFT   |     Z     |     X     |     C     |     D     |     V     |          |      K     |     H    |     ,     |     .     |     /     |    SHFT    |
// ------------------------------------------------------------------------------------------------------------------------------------------------------------
//                                    |    GUI    |   LGUI    |    LWR    |          |     RSE    |    SPA   |   RCTRL   |
//                                    -------------------------------------          -------------------------------------
                bindings = <
&kp TAB     &kp Q   &kp W   &kp F   &kp P   &kp B       &kp J   &kp L   &kp U       &kp Y       &kp SEMI    &kp BSPC
&kp LCTRL   &kp A   &kp R   &kp S   &kp T   &kp G       &kp M   &kp N   &kp E       &kp I       &kp O       &kp RET
&kp LSHFT   &kp Z   &kp X   &kp C   &kp D   &kp V       &kp K   &kp H   &kp COMMA   &kp DOT     &kp FSLH    &kp RSHFT
                            &kp LGUI    &kp LGUI    &mo 1   &kp SPACE   &mo 2   &kp RCTRL
>;
};

                lower_layer {
// -------------------------------------------------------------------------------------------------------------------------------------------------------------
// |           |           |           |           |           |           |          |            |          |     UP    |           |           |            |
// ------------------------------------------------------------------------------------------------------------------------------------------------------------
// |           |           |   LCTRL   |    LALT   |    LGUI   |           |          |            |   LEFT   |    DWN    |   RIGHT   |           |
// ------------------------------------------------------------------------------------------------------------------------------------------------------------|
// |   SHFT    |           |           |           |           |           |          |            |          |           |           |           |            |
// ------------------------------------------------------------------------------------------------------------------------------------------------------------
//                                     |           |           |           |          |            |          |           |
//                                     -------------------------------------          -------------------------------------
                bindings = <
&trans      &trans      &trans      &trans      &trans        &trans  &kp UP  &trans  &trans  &trans  &kp BSPC
&trans      &kp RCTRL   &kp LALT    &kp LGUI    &trans        &trans  &kp LFT &kp DWN &kp RGT &trans  &trans
&kp LSHFT   &trans      &trans      &trans      &trans        &trans  &trans  &trans  &trans  &trans  &trans
                                        &kp LGUI    &trans  &kp SPACE       &kp RET     &trans  &kp RALT
                                          >;
    };

                raise_layer {

// -------------------------------------------------------------------------------------------------------------------------------------------------------------
// |           |           |           |           |           |           |          |            |          |     UP    |           |           |            |
// ------------------------------------------------------------------------------------------------------------------------------------------------------------
// |           |           |   LCTRL   |    LALT   |    LGUI   |           |          |            |   LEFT   |    DWN    |   RIGHT   |           |
// ------------------------------------------------------------------------------------------------------------------------------------------------------------|
// |   SHFT    |           |           |           |           |           |          |            |          |           |           |           |            |
// ------------------------------------------------------------------------------------------------------------------------------------------------------------
//                                     |           |           |           |          |            |          |           |
//                                     -------------------------------------          -------------------------------------
// -----------------------------------------------------------------------------------------
// |  TAB |  !  |  @  |  #  |  $  |  %  |   |  ^  |  &  |  *  |  (  |  )  | BSPC |
// | CTRL |     |     |     |     |     |   |  -  |  =  |  [  |  ]  |  \  |  `   |
// | SHFT |     |     |     |     |     |   |  _  |  +  |  {  |  }  | "|" |  ~   |
//                    | GUI |     | SPC |   | ENT |     | ALT |
                        bindings = <
    &kp TAB   &kp EXCL  &kp AT     &kp HASH   &kp DLLR   &kp PRCNT               &kp CARET  &kp AMPS   &kp ASTRK  &kp LPAR   &kp RPAR   &kp BSPC
    &kp LCTRL &trans    &trans     &trans     &trans     &trans                  &kp MINUS  &kp EQUAL  &kp LBKT   &kp RBKT   &kp BSLH   &kp GRAVE
    &kp LSHFT &trans    &trans     &trans     &trans     &trans                  &kp UNDER  &kp PLUS   &kp LBRC   &kp RBRC   &kp PIPE   &kp TILDE
                                 &kp LGUI    &trans     &kp SPACE               &kp RET    &trans     &kp RALT
                              >;
        };
};
